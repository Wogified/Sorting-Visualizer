{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": 0,
    "react/jsx-fragments": ["off", "element"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
